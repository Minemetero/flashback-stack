{
    "name": "flashback-stack",
    "version": "1.0.0",
    "description": "Tiny timeâ€‘travel (undo/redo) stack",
    "type": "module",
    "publishConfig": {
        "access": "public",
        "git-checks": false
    },
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --target es2022 --dts",
        "dev": "tsup src/index.ts --watch --format esm --target es2022 --dts",
        "test": "vitest run",
        "prepare": "npm run build",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "keywords": [
        "undo",
        "redo",
        "stack",
        "esm",
        "typescript"
    ],
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.31.0",
        "@typescript-eslint/parser": "^8.31.0",
        "@vitest/coverage-v8": "^3.1.2",
        "eslint": "^9.25.1",
        "prettier": "^3.5.3",
        "tsup": "^8.4.0",
        "typescript": "^5.8.3",
        "vitest": "^3.1.2"
    }
}
